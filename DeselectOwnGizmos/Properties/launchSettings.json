{
  "profiles": {

    "Launch Mod": {
      "commandName": "Executable",
      "executablePath": "$(AppPath)$(AppName).exe",
      "commandLineArgs": "-Screen -Invisible -LoadAssembly \"Libraries/$(AppName)ModLoader.dll\" -DoNotAutoLoadHome -SkipIntroTutorial",
      "workingDirectory": "$(AppPath)"
    },
    "Launch Mod in VR": {
      "commandName": "Executable",
      "executablePath": "$(AppPath)$(AppName).exe",
      "commandLineArgs": "-Invisible -LoadAssembly \"Libraries/$(AppName)ModLoader.dll\" -DoNotAutoLoadHome -SkipIntroTutorial",
      "workingDirectory": "$(AppPath)"
    },
    "Launch Mod Alt": {
      "commandName": "Executable",
      "executablePath": "$(AppPath)$(AppName).exe",
      "commandLineArgs": "-Screen -Invisible -LoadAssembly \"Libraries/$(AppName)ModLoader.dll\" -DoNotAutoLoadHome -SkipIntroTutorial -Config Config.json -DataPath \"$(AlternateDataPath)\\Data\" -CachePath \"$(AlternateDataPath)\\Cache\"",
      "workingDirectory": "$(AppPath)"
    },
    "Launch Mod in VR Alt": {
      "commandName": "Executable",
      "executablePath": "$(AppPath)$(AppName).exe",
      "commandLineArgs": "-Invisible -LoadAssembly \"Libraries/$(AppName)ModLoader.dll\" -DoNotAutoLoadHome -SkipIntroTutorial -Config Config.json -DataPath \"$(AlternateDataPath)\\Data\" -CachePath \"$(AlternateDataPath)\\Cache\"",
      "workingDirectory": "$(AppPath)"
    }
  }
}